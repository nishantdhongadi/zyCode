{
  "name": "zycode",
  "displayName": "zyCode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:workbench.action.files.save"
  ],
  "main": "./out/extension.js",
